{
    "name": "pterobilling/pterobilling",
    "description": "An open-source Laravel 8 online store, client area, and billing software specially made for Pterodactyl panel",
    "homepage": "https://pterobilling.org",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Alaister Leung",
            "email": "contact@alaister.net",
            "homepage": "https://about.alaister.net",
            "role": "Developer"
        },
        {
            "name": "MinePlay85",
            "role": "Collaborator"
        },
        {
            "name": "Louis Bailleau",
            "email": "louis.bailleau@free.fr",
            "homepage": "https://bricklou.info",
            "role": "Collaborator"
        }
    ],
    "support": {
        "email": "alaister@pterobilling.org",
        "issues": "https://github.com/pterobilling/pterobilling/issues",
        "forum": "https://github.com/pterobilling/pterobilling/discussions",
        "source": "https://github.com/pterobilling/pterobilling",
        "docs": "https://docs.pterobilling.org"
    },
    "funding": [
        {
            "type": "paypal",
            "url": "https://paypal.me/alaisterleung"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pterobilling/pterodactyl-sdk"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "cerbero/command-validator": "^2.5",
        "cloudflare/sdk": "^1.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^8.34",
        "laravel/tinker": "^2.6",
        "predis/predis": "^1.1",
        "pterobilling/pterodactyl-sdk": "dev-master",
        "queueworker/sansdaemon": "^1.2",
        "srmklive/paypal": "~1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9",
        "laravel/sail": "^1.4",
        "laravel/telescope": "^4.4",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.3",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Extensions\\": "extensions/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
